      "39": "RIGHT",
      "40": "DOWN",
      "250": "PLAY",
      "19": "PAUSE",
      "178": "STOP",
      "228": "FAST_FWD",
      "227": "REWIND",
      "8": "BACK",
      "62821": "RED",
      "62822": "GREEN",
      "62823": "YELLOW",
