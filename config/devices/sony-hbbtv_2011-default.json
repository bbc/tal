{
  "pageStrategy": "hbbtv",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/noanim",
      "antie/devices/media/cehtml",
      "antie/devices/net/default",
      "antie/devices/data/json2",
      "antie/devices/storage/cookie",
      "antie/devices/logging/onscreen",
      "antie/devices/logging/xhr",
      "antie/devices/logging/jstestdriver",
      "antie/devices/exit/history"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "hls",
            "plain"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ]
        }
      ]
    }
  },
  "input": {
    "map": {
      "13": "ENTER",
      "48": "0",
      "49": "1",
      "50": "2",
      "51": "3",
      "52": "4",
      "53": "5",
      "54": "6",
      "55": "7",
      "56": "8",
      "57": "9",
      "473": "FAST_FWD",
      "412": "REWIND",
      "415": "PLAY",
      "19": "PAUSE",
      "413": "STOP",
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "467": "SUBTITLE",
      "73": "INFO",
      "461": "BACK"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 960,
      "height": 540,
      "module": "%application%/appui/layouts/540p",
      "classes": [
        "browserdevice540p"
      ]
    },
    {
      "width": 1280,
      "height": 720,
      "module": "%application%/appui/layouts/720p",
      "classes": [
        "browserdevice720p"
      ]
    },
    {
      "width": 1920,
      "height": 1080,
      "module": "%application%/appui/layouts/1080p",
      "classes": [
        "browserdevice1080p"
      ]
    }
  ],
  "networking": {
    "supportsJSONP": true
  }
}