{
  "pageStrategy": "html5",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/noanim",
      "antie/devices/media/cehtml",
      "antie/devices/net/default",
      "antie/devices/data/nativejson",
      "antie/devices/logging/onscreen",
      "antie/devices/logging/xhr",
      "antie/devices/logging/jstestdriver",
      "antie/devices/storage/cookie"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "hls",
            "plain"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ],
          "maximumBitRate": 192
        }
      ]
    }
  },
  "input": {
    "map": {
      "38": "UP",
      "40": "DOWN",
      "37": "LEFT",
      "39": "RIGHT",
      "13": "ENTER",
      "2097162": "PLAY_PAUSE",
      "2097157": "PLAY",
      "19": "PAUSE",
      "2097158": "STOP",
      "2097164": "FAST_FWD",
      "2097166": "REWIND",
      "2097167": "INFO",
      "1572866": "BACK",
      "2097181": "SUBTITLE"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 960,
      "height": 540,
      "module": "%application%/appui/layouts/540p",
      "classes": [
        "browserdevice540p"
      ]
    },
    {
      "width": 1280,
      "height": 720,
      "module": "%application%/appui/layouts/720p",
      "classes": [
        "browserdevice720p"
      ]
    },
    {
      "width": 1920,
      "height": 1080,
      "module": "%application%/appui/layouts/1080p",
      "classes": [
        "browserdevice1080p"
      ]
    }
  ],
  "networking": {
    "supportsJSONP": true
  }
}