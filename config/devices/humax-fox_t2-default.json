{
  "pageStrategy": "html5",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/styletopleft",
      "antie/devices/media/html5",
      "antie/devices/net/default",
      "antie/devices/data/nativejson",
      "antie/devices/logging/default",
      "antie/devices/storage/cookie",
      "antie/devices/exit/history"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "hls",
            "plain"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "encodings": [
            "aac"
          ],
          "protocols": [
            "http"
          ],
          "maximumBitRate": 192
        }
      ]
    }
  },
  "input": {
    "map": {
      "38": "UP",
      "40": "DOWN",
      "37": "LEFT",
      "39": "RIGHT",
      "13": "ENTER",
      "415": "PLAY",
      "19": "PAUSE",
      "413": "STOP",
      "417": "FAST_FWD",
      "412": "REWIND",
      "461": "BACK",
      "460": "SUBTITLE",
      "457": "INFO",
      "403": "RED",
      "404": "GREEN",
      "405": "YELLOW",
      "406": "BLUE",
	  "48" : "0",
	  "49" : "1",
	  "50" : "2",
	  "51" : "3",
	  "52" : "4",
	  "53" : "5",
	  "54" : "6",
	  "55" : "7",
	  "56" : "8",
	  "57" : "9"	  
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 960,
      "height": 540,
      "classes": [
        "browserdevice540p"
      ],
      "module": "%application%/appui/layouts/540p"
    },
    {
      "width": 1280,
      "height": 720,
      "classes": [
        "browserdevice720p"
      ],
      "module": "%application%/appui/layouts/720p"
    }
  ],
  "networking": {
    "supportsJSONP": true
  }
}