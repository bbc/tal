{
  "pageStrategy": "hbbtv",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/noanim",
      "antie/devices/mediaplayer/cehtml",
      "antie/devices/mediaplayer/live/none",
      "antie/devices/storage/cookie",
      "antie/devices/exit/closewindow",
      "antie/devices/parentalguidance/appdefaultpghandler"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "hls",
            "plain"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "encodings": [
            "aac"
          ],
          "protocols": [
            "http"
          ],
          "maximumBitRate": 192
        }
      ]
    }
  },
  "input": {
    "map": {
      "13": "ENTER",
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "57396": "BACK_SPACE",
      "32": "SPACE",
      "461": "BACK",
      "415": "PLAY",
      "19": "PAUSE",
      "413": "STOP",
      "473": "FAST_FWD",
      "412": "REWIND",
      "460": "SUBTITLES",
      "455": "INFO",
      "403": "RED",
      "404": "GREEN",
      "57856": "YELLOW",
      "406": "BLUE"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 960,
      "height": 540,
      "classes": [
        "browserdevice540p"
      ],
      "module": "%application%/appui/layouts/540p"
    },
    {
      "width": 1280,
      "height": 720,
      "classes": [
        "browserdevice720p"
      ],
      "module": "%application%/appui/layouts/720p"
    }
  ],
  "networking": {
    "supportsJSONP": true
  },
  "capabilities": [],
  "statLabels": {
    "deviceType": "smarttv",
    "serviceType": "retail",
    "browserType": "opera"
  }
}
