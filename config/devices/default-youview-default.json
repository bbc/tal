{
    "pageStrategy": "html5hbbtvhybrid",
    "modules": {
        "base": "antie/devices/browserdevice",
        "modifiers": [
            "antie/devices/anim/noanim",
            "antie/devices/media/html5memoryleakfix",
      "antie/devices/mediaplayer/html5",
      "antie/devices/mediaplayer/live/seekable",
            "antie/devices/net/default",
            "antie/devices/data/nativejson",
            "antie/devices/broadcastsource/hbbtvsource",
            "antie/devices/storage/cookie",
            "antie/devices/logging/consumelog",
            "antie/devices/exit/closewindow",
            "antie/devices/parentalguidance/youviewpghandler"
        ]
    },
    "logging": {
        "level": "none"
    },
    "statReporters": [
        "tvpjslib/stats/avreporters/youviewreporter"
    ],
    "streaming": {
        "video": {
            "mediaURIFormat": "%href%",
            "supported": [
                {
                    "protocols": [
                        "http"
                    ],
                    "encodings": [
                        "h264"
                    ],
                    "transferFormat": ["hls"],
                    "maximumVideoLines": 1080
                }
            ]
        },
        "audio": {
            "mediaURIFormat": "%href%",
            "supported": [
                {
                    "encodings": [
                        "aac"
                    ],
                    "protocols": [
                        "http"
                    ],
                    "maximumBitRate": 192
                }
            ]
        }
    },
    "input": {
        "map": {
            "417": "FAST_FWD",
            "425": "NEXT",
            "412": "REWIND",
            "424": "PREV",
            "413": "STOP",
            "415": "PLAY",
            "19": "PAUSE",
            "8": "BACK",
            "32": "SPACE",
            "460": "SUBTITLE",
            "40": "DOWN",
            "79": "PAUSE",
            "13": "ENTER",
            "39": "RIGHT",
            "38": "UP",
            "37": "LEFT",
            "48": "0",
            "49": "1",
            "50": "2",
            "51": "3",
            "52": "4",
            "53": "5",
            "54": "6",
            "55": "7",
            "56": "8",
            "57": "9",
            "457": "INFO",
            "57347": "AUDIODESCRIPTION",
            "403": "RED",
            "404": "GREEN",
            "405": "YELLOW",
            "406": "BLUE",
            "156": "HELP",
            "170": "SEARCH"
        }
    },
    "accessibility": {
        "captions": {
            "supported": [
                "application/ttaf+xml"
            ]
        }
    },
    "layouts": [
        {
            "width": 960,
            "height": 540,
            "module": "%application%/appui/layouts/540p",
            "classes": [
                "browserdevice540p"
            ]
        },
        {
            "width": 1280,
            "height": 720,
            "module": "%application%/appui/layouts/720p",
            "classes": [
                "browserdevice720p"
            ]
        },
        {
            "width": 1920,
            "height": 1080,
            "module": "%application%/appui/layouts/1080p",
            "classes": [
                "browserdevice1080p"
            ]
        }
    ],
    "networking": {
        "supportsJSONP": true,
        "supportsCORS": true
    }
}
