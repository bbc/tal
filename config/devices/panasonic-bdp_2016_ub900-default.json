{
  "pageStrategy": "html5",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/css3",
      "antie/devices/mediaplayer/html5",
      "antie/devices/mediaplayer/live/seekable",
      "antie/devices/storage/cookie",
      "antie/devices/exit/closewindow",
      "antie/devices/parentalguidance/appdefaultpghandler"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "dash",
            "hls",
            "plain"
          ],
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ],
          "maximumBitRate": 192
        }
      ]
    }
  },
  "input": {
    "map": {
      "38": "UP",
      "40": "DOWN",      
      "37": "LEFT",    
      "39": "RIGHT",
      "13": "ENTER",
      "403": "RED",
      "113": "PLAY",
      "114": "PAUSE",
      "115": "STOP",
      "116": "NEXT",
      "117": "PREV",
      "116": "FAST_FWD",
      "117": "REWIND",
      "48": "0",
      "49": "1",
      "50": "2",
      "51": "3",
      "52": "4",
      "53": "5",
      "54": "6",
      "55": "7",
      "56": "8",
      "57": "9",
      "166": "BACK",
      "457":"INFO",
      "404": "GREEN",
      "405": "YELLOW",
      "406": "BLUE",
      "112": "SUBTITLE"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 1280,
      "height": 720,
      "module": "%application%/appui/layouts/720p",
      "classes": [
        "browserdevice720p"
      ]
    },
    {
      "width": 1920,
      "height": 1080,
      "module": "%application%/appui/layouts/1080p",
      "classes": [
        "browserdevice1080p"
      ]
    }
  ],
  "networking": {
    "supportsJSONP": true,
    "supportsCORS": true
  },
  "capabilities": ["dial_capable"],
  "statLabels": {
    "deviceType": "bd",
    "serviceType": "retail",
    "browserType": "Webkit"
  }
}
