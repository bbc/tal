{
  "pageStrategy": "html5hbbtvhybrid",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/noanim",
      "antie/devices/mediaplayer/html5",
      "antie/devices/mediaplayer/live/playable",
      "antie/devices/broadcastsource/hbbtvsource",
      "antie/devices/exit/history",
      "antie/devices/storage/cookie",
      "antie/devices/parentalguidance/appdefaultpghandler"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "hls",
            "plain"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ]
        }
      ]
    }
  },
  "input": {
    "map": {
      "13": "ENTER",
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "19": "PAUSE",
      "19": "PLAY_PAUSE",
      "415": "PLAY",
      "413": "STOP",
      "473": "FAST_FWD",
      "412": "REWIND",
      "461": "BACK",
      "458": "SUBTITLE",
      "455": "INFO",
      "48": "0",
      "49": "1",
      "50": "2",
      "51": "3",
      "52": "4",
      "53": "5",
      "54": "6",
      "55": "7",
      "56": "8",
      "57": "9",
	  "403": "RED",
	  "404": "GREEN",
	  "502": "YELLOW",
	  "406": "BLUE"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts":   [
    {
      "width": 960,
      "height": 540,
      "classes": [
        "browserdevice540p"
      ],
      "module": "%application%/appui/layouts/540p"
    },
    {
      "width": 1280,
      "height": 720,
      "classes": [
        "browserdevice720p"
      ],
      "module": "%application%/appui/layouts/720p"
    },
    {
      "width": 1920,
      "height": 1080,
      "classes": [
        "browserdevice1080p"
      ],
      "module": "%application%/appui/layouts/1080p"
    }
   ],
  "networking": {
    "supportsJSONP": true
  },
  "broadcast" : {
    "currentChannelValidation": {
      "enabled": true
    }
  },
  "capabilities": [],
  "statLabels": {
    "deviceType": "stb",
    "serviceType": "retail"
  }
}
