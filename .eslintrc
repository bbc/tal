{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "curly": 2,
        "no-console": 0,
        "no-constant-condition": 0,
        "no-empty": 0,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-new-wrappers": 2,
        "brace-style": 2,
        "no-eval": 2,
        "no-implied-eval": 2
    },
    "globals" :{
        "require": true,
        "ActiveXObject": true,
        "jQuery": true,
        "console": true,
        "module": true,
        "document": true,
        "window": true,
        "Image": true,
        "XMLHttpRequest": true,
        "TizenTVSource": true,
        "tivo": true,
        "tizen": true,
        "setTimeout": true,
        "clearTimeout": true,
        "clearInterval": true,
        "navigator": true,
        "setInterval": true,
        "define": true,
        "HTMLMediaElement": true,
        "alert": true,
        "antie": true,
        "history": true,
        "unescape": true,
        "DOMParser": true,
        "sinon": true,
        "fail": true,
        "assert": true,
        "expectAsserts": true,
        "assertEquals": true,
        "queuedRequire": true,
        "queuedApplicationInit": true,
        "queuedComponentInit": true,
        "assertClassName": true,
        "assertSame": true,
        "assertNotEquals": true,
        "assertException": true,
        "assertObject": true,
        "assertFalse": true,
        "assertTrue": true,
        "assertNull": true,
        "assertInstanceOf": true,
        "assertNoException": true,
        "assertMatch": true,
        "assertNotNull": true,
        "assertNotSame": true,
        "assertUndefined": true,
        "assertFunction": true,
        "assertNotUndefined": true,
        "assertBoolean": true,
        "assertNoMatch": true,
        "AsyncTestCase": true,
        "jstestdriver": true,
        "it": true,
        "spyOn": true,
        "jasmine": true,
        "expect": true,
        "beforeEach": true,
        "afterEach": true,
        "describe": true,
        "onDeviceTestConfigValidation": true
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "jasmine",
        "requirejs"
    ]
}
